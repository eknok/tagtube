<h1>Toppages#index</h1>
<p>Find me in app/views/toppages/index.html.erb</p>
<div class="search_results">
  <% i = 9 %>
  <% @results.items.each do |item| %>
    <% hoge = item.id %>
    <% if i == 9 %>
      <iframe class="iframe-left" width="596" height="404" src="https://www.youtube.com/embed/<%= hoge.video_id %>" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
      <% i = 1 %>
    <% else %>
      <iframe class="iframe-left" width="295" height="200" src="https://www.youtube.com/embed/<%= hoge.video_id %>" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
      <% i += 1 %>
    <% end %>
  <% end %>
</div>
